class Solution {
public:
    int maxArea(vector<int>& height) {
        int marea=0;
        int i=0;
        int j=height.size()-1;
        while(i<j){
            int area=min(height[i],height[j])*(j-i);
            marea=max(area,marea);
            if(height[i]<height[j]){
                i++;
            }
            else{
                j--;
            }
        }
        return marea;
    }
};
